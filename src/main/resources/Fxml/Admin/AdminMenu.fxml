<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="750.0" prefWidth="180.0" stylesheets="@../../Styles/ClienMenu.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Controllers.Admin.AdminMenuController">
   <children>
      <AnchorPane prefHeight="835.0" prefWidth="180.0" styleClass="left_container">
         <children>
            <VBox prefHeight="80.0" prefWidth="180.0" styleClass="title_container" stylesheets="@../../Styles/ClienMenu.css" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <FontAwesomeIconView glyphName="BANK" size="30" text="" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="KroliBank" />
               </children>
            </VBox>
            <VBox layoutX="14.0" layoutY="137.0" prefHeight="400.0" prefWidth="180.0" styleClass="menu_container" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="100.0">
               <children>
                  <Button fx:id="create_client_btn" mnemonicParsing="false" text="Create Client">
                     <graphic>
                        <FontAwesomeIconView glyphName="PLUS" size="20" text="" />
                     </graphic>
                  </Button>
                  <Button fx:id="clients_btn" mnemonicParsing="false" text="Clients">
                     <graphic>
                        <FontAwesomeIconView glyphName="LIST" size="20" />
                     </graphic>
                  </Button>
                  <Button fx:id="deposit_btn" mnemonicParsing="false" text="Deposit">
                     <graphic>
                        <FontAwesomeIconView glyphName="MONEY" size="20" />
                     </graphic>
                  </Button>
                  <Button fx:id="logout_btn" mnemonicParsing="false" styleClass="alt_menu_btn" text="Logout">
                     <graphic>
                        <FontAwesomeIconView glyphName="EXTERNAL_LINK" size="20" />
                     </graphic>
                  </Button>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <VBox prefHeight="80.0" prefWidth="160.0" />
   </children>
</VBox>
